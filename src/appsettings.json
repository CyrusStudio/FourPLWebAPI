{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Hangfire": "Information"
    }
  },
  "AllowedHosts": "*",
  "UseMockSap": false,
  "EnableSwaggerInProduction": false,
  "ConnectionStrings": {
    "DefaultConnection": "Server=YOUR_SQL_SERVER;Database=FourPL;User Id=YOUR_USER;Password=YOUR_PASSWORD;TrustServerCertificate=True;",
    "HangfireConnection": "Server=YOUR_SQL_SERVER;Database=FourPL;User Id=YOUR_USER;Password=YOUR_PASSWORD;TrustServerCertificate=True;"
  },
  "Hangfire": {
    "DashboardPath": "/hangfire",
    "ScheduleSyncIntervalMinutes": 5
  },
  "Sap": {
    "AppServerHost": "YOUR_SAP_SERVER",
    "SystemNumber": "00",
    "SystemId": "PRD",
    "User": "YOUR_SAP_USER",
    "Password": "YOUR_SAP_PASSWORD",
    "Client": "100",
    "Language": "ZH"
  },
  "Sftp": {
    "Host": "YOUR_SFTP_SERVER",
    "Port": 22,
    "Username": "YOUR_SFTP_USER",
    "Password": "YOUR_SFTP_PASSWORD",
    "PrivateKeyPath": ""
  }
}